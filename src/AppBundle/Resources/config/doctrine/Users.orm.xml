<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="Users">
    <unique-constraints>
      <unique-constraint name="Users_LoginU" columns="Login"/>
      <unique-constraint name="Users_Password_HashU" columns="Password_Hash"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="login" type="string" column="Login" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passwordHash" type="string" column="Password_Hash" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isAdmin" type="integer" column="Is_Admin" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="Date_Created" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
