<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Cities" table="Cities">
    <indexes>
      <index name="Cities_ID_COUNTRIES_FK" columns="ID_COUNTRIES"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="Cities_Name_CountriesU" columns="Name,ID_COUNTRIES"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="Name" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="note" type="string" column="Note" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idCountries" target-entity="Countries" fetch="LAZY">
      <join-columns>
        <join-column name="ID_COUNTRIES" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
