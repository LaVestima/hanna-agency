<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Producers" table="Producers">
    <indexes>
      <index name="FK_160DB71E741523F5" columns="ID_CITIES"/>
      <index name="FK_160DB71E73C1D4A" columns="ID_COUNTRIES"/>
      <index name="FK_160DB71E3B997DA3" columns="ID_USERS"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="shortName" type="string" column="Short_Name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fullName" type="string" column="Full_Name" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="First_Name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="Last_Name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vat" type="string" column="VAT" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="street" type="string" column="Street" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postalCode" type="string" column="Postal_Code" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="Email" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="Phone" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idUsers" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="ID_USERS" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCountries" target-entity="Countries" fetch="LAZY">
      <join-columns>
        <join-column name="ID_COUNTRIES" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCities" target-entity="Cities" fetch="LAZY">
      <join-columns>
        <join-column name="ID_CITIES" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
